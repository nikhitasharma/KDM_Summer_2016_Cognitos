[Matei Zaharia, Mosharaf Chowdhury, Michael J. Franklin, Scott Shenker, Ion Stoica University of California, Berkeley,WrappedArray(matei, zaharia,, mosharaf, chowdhury,, michael, j., franklin,, scott, shenker,, ion, stoica, university, of, california,, berkeley),WrappedArray(matei, zaharia,, mosharaf, chowdhury,, michael, j., franklin,, scott, shenker,, ion, stoica, university, california,, berkeley),(20,[2,3,5,8,10,12,18,19],[1.0,1.0,1.0,2.0,1.0,4.0,1.0,3.0]),(20,[2,3,5,8,10,12,18,19],[1.1298648321722142,1.1786549963416462,1.0608719606852626,2.459896581458393,1.0388930539664876,0.7231371099002725,1.216876209161844,3.6506286274855317])]
